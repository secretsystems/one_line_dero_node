echo -e "apt install wget \npwd \nwget https://github.com/deroproject/derohe/releases/latest/download/dero_linux_arm64.tar.gz \ntar xvzf dero_linux_arm64.tar.gz \nrm dero_linux_arm64.tar.gz -f \ncd dero_linux_arm64 \npwd \n./derod-linux-arm64" > installderonode.sh | chmod +x installderonode.sh && ./installderonode.sh
